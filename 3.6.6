my_list = [1,2,4,4,1,4,2,6,2,9]

# Step 2: Create an empty list to store  numbers
empty_list = []

# Step 3: check through the list
for number in my_list:

    # Step 4: Check if the number is already in the empty list
    if number not in empty_list:
    
        # Step 5: If not, append it to the unique list
        empty_list.append(number)

# Display the list with duplicates removed
print("List with duplicates removed:", empty_list)
